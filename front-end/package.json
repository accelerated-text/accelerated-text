{
  "private": true,
  "name": "accelerated-text-front-end",
  "version": "1.0.0",
  "description": "Accelerated Text Front-End",
  "main": "index.js",
  "scripts": {
    "build": "node -r esm node_modules/.bin/webpack --config packages/webpack/build.config.js",
    "build-dev": "node -r esm node_modules/.bin/webpack --config packages/webpack/build.dev.config.js",
    "check:audit": "npm audit || echo 'Fix the issues above.'",
    "start": "node -r esm node_modules/.bin/webpack-dev-server --host 0.0.0.0 --config packages/webpack/run.config.js",
    "test": "npm run test:eslint && npm run test:sasslint && npm run test:ava && npm run check:audit",
    "test:ava": "node -r esm packages/tests/lib/run-ava",
    "test:ava:serial": "node -r esm packages/tests/lib/run-ava --serial",
    "test:eslint": "eslint packages",
    "test:sasslint": "sass-lint -vq"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:tokenmill/nlg/accelerated-text.git"
  },
  "keywords": [
    "text generation",
    "NLG"
  ],
  "author": "Tokenmill.lt",
  "license": "UNLICENSED",
  "dependencies": {
    "apollo-boost": "^0.4.3",
    "bw-color-blind-palette": "^1.0.0",
    "check-prop-types": "^1.1.2",
    "classnames": "^2.2.6",
    "debug": "^4.1.1",
    "dom-dom": "^1.2.3",
    "dotenv-extended": "^2.3.0",
    "fetch-inject": "^2.0.2",
    "flux": "^3.1.3",
    "graphql": "^14.3.0",
    "graphql-tag": "^2.10.1",
    "isomorphic-unfetch": "^3.0.0",
    "lodash.debounce": "^4.0.8",
    "marked": "^4.0.10",
    "p-tap": "^2.0.0",
    "preact": "^10.1.0",
    "preact-render-to-string": "^5.0.3",
    "preact-router": "^3.2.1",
    "prop-types": "^15.6.2",
    "ramda": "^0.26.1",
    "react-apollo": "^2.5.8",
    "react-debounced-props": "^0.1.0",
    "react-simple-resizer": "^1.3.1",
    "sass-mix-ratio": "^0.0.1",
    "shallow-equal": "^1.0.0",
    "shortkey": "0.0.1",
    "uuid": "^3.3.2"
  },
