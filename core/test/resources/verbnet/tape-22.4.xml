    <THEMROLES>
        <!--Patient is interpreted as Figure, Co-Patient as Ground-->
        <!--Each verb specifies an Instrument-->
        <THEMROLE type="Agent">
            <SELRESTRS logic="or">
                <SELRESTR Value="+" type="animate"/>
                <SELRESTR Value="+" type="machine"/>
            </SELRESTRS>
        </THEMROLE>
        <THEMROLE type="Patient">
            <SELRESTRS>
                <SELRESTR Value="+" type="solid"/>
            </SELRESTRS>
        </THEMROLE>
        <THEMROLE type="Co-Patient">
            <SELRESTRS>
                <SELRESTR Value="+" type="solid"/>
            </SELRESTRS>
        </THEMROLE>
        <THEMROLE type="Instrument">
            <SELRESTRS>
                <SELRESTR Value="+" type="concrete"/>
                <SELRESTR Value="-" type="animate"/>
            </SELRESTRS>
        </THEMROLE>
        <!--Instrument is realized in the verb-->
        <THEMROLE type="Result">
            <SELRESTRS/>
        </THEMROLE>
    </THEMROLES>
