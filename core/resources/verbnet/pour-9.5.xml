                <PREP>
                    <SELRESTRS>
                        <SELRESTR Value="+" type="src"/>
                    </SELRESTRS>
                </PREP>
                <NP value="Initial_Location">
                    <SYNRESTRS/>
                </NP>
                <PREP>
                    <SELRESTRS>
                        <SELRESTR Value="+" type="dest_conf"/>
                    </SELRESTRS>
                </PREP>
                <NP value="Destination">
                    <SYNRESTRS/>
                </NP>
            </SYNTAX>
            <SEMANTICS>
