<!DOCTYPE VNCLASS SYSTEM "vn_class-3.dtd">
<VNCLASS xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" ID="put_direction-9.4" xsi:noNamespaceSchemaLocation="vn_schema-3.xsd">
    <MEMBERS>
        <!--Direction for this class is "against gravity"-->
        <MEMBER name="drop" wn="drop%2:38:01" grouping="drop.01 drop.02         drop.04"/>
        <MEMBER name="drive" wn="drive%2:35:06" grouping="drive.05"/>
        <MEMBER name="elevate" wn="elevate%2:38:00" grouping="elevate.02 elevate.04"/>
        <MEMBER name="heel" wn="heel%2:38:01" grouping="heel.01"/>
        <MEMBER name="hoist" wn="hoist%2:35:00 hoist%2:35:10 hoist%2:35:01" grouping=""/>
        <MEMBER name="lift" wn="lift%2:38:00 lift%2:35:00 lift%2:35:01" grouping="lift.01"/>
        <MEMBER name="lower" wn="lower%2:38:00" grouping="lower.01"/>
        <MEMBER name="raise" wn="raise%2:38:00" grouping="raise.02"/>
        <MEMBER name="back" wn="back%2:38:01" grouping=""/>
    </MEMBERS>
    <THEMROLES>
        <!--Destination can also be realized as a locative adverb ("here, there")
        Levin doesn't include Instrument or Instrument Subject Alternation ("the crane lifted the beam over the building")-->
        <THEMROLE type="Agent">
            <SELRESTRS>
                <SELRESTR Value="+" type="animate"/>
            </SELRESTRS>
        </THEMROLE>
        <THEMROLE type="Theme">
            <SELRESTRS>
                <SELRESTR Value="+" type="concrete"/>
            </SELRESTRS>
        </THEMROLE>
        <THEMROLE type="Initial_Location">
            <SELRESTRS>
                <SELRESTR Value="+" type="location"/>
            </SELRESTRS>
        </THEMROLE>
        <THEMROLE type="Destination">
            <SELRESTRS>
                <SELRESTR Value="+" type="location"/>
                <SELRESTR Value="-" type="region"/>
            </SELRESTRS>
        </THEMROLE>
        <!--Exception: If Destination is object of a source-type preposition ("from"), then +region is allowed-->
    </THEMROLES>
    <FRAMES>
        <FRAME>
            <DESCRIPTION descriptionNumber="0.2" primary="NP V NP" secondary="Basic Transitive" xtag="0.2"/>
            <EXAMPLES>
                <EXAMPLE>I lifted the books.</EXAMPLE>
            </EXAMPLES>
            <SYNTAX>
                <NP value="Agent">
                    <SYNRESTRS/>
                </NP>
                <VERB/>
                <NP value="Theme">
                    <SYNRESTRS/>
                </NP>
            </SYNTAX>
            <SEMANTICS>
                <PRED value="motion">
                    <ARGS>
                        <ARG type="Event" value="during(E)"/>
                        <ARG type="ThemRole" value="Theme"/>
                    </ARGS>
                </PRED>
                <PRED value="path_rel">
                    <ARGS>
                        <ARG type="Event" value="start(E)"/>
                        <ARG type="ThemRole" value="Theme"/>
                        <ARG type="ThemRole" value="?Initial_Location"/>
                        <ARG type="Constant" value="ch_of_loc"/>
                        <ARG type="VerbSpecific" value="prep"/>
                    </ARGS>
                </PRED>
                <PRED value="path_rel">
                    <ARGS>
                        <ARG type="Event" value="end(E)"/>
                        <ARG type="ThemRole" value="Theme"/>
                        <ARG type="ThemRole" value="?Destination"/>
                        <ARG type="Constant" value="ch_of_loc"/>
                        <ARG type="VerbSpecific" value="prep"/>
                    </ARGS>
                </PRED>
                <PRED value="exert_force">
                    <ARGS>
                        <ARG type="Event" value="during(E)"/>
                        <ARG type="ThemRole" value="Agent"/>
                        <ARG type="ThemRole" value="Theme"/>
                    </ARGS>
                </PRED>
                <PRED value="direction">
                    <ARGS>
                        <ARG type="Event" value="during(E)"/>
                        <ARG type="Constant" value="toward"/>
                        <ARG type="ThemRole" value="Theme"/>
                        <ARG type="ThemRole" value="Destination"/> 
                    </ARGS>
                </PRED>
                <PRED value="cause">
                    <ARGS>
                        <ARG type="ThemRole" value="Agent"/>
                        <ARG type="Event" value="E"/>
                    </ARGS>
                </PRED>
            </SEMANTICS>
        </FRAME>
        <FRAME>
            <DESCRIPTION descriptionNumber="0.2" primary="NP V NP PP.destination" secondary="NP-PP; locative-PP" xtag=""/>
            <EXAMPLES>
                <EXAMPLE>I lifted the books onto the table.</EXAMPLE>
            </EXAMPLES>
            <SYNTAX>
                <NP value="Agent">
                    <SYNRESTRS/>
                </NP>
                <VERB/>
                <NP value="Theme">
                    <SYNRESTRS/>
                </NP>
                <PREP>
                    <SELRESTRS>
                        <SELRESTR Value="+" type="spatial"/>
                    </SELRESTRS>
                </PREP>
                <NP value="Destination">
                    <SYNRESTRS/>
                </NP>
            </SYNTAX>
            <SEMANTICS>
                <PRED value="motion">
                    <ARGS>
                        <ARG type="Event" value="during(E)"/>
                        <ARG type="ThemRole" value="Theme"/>
                    </ARGS>
                </PRED>
                <PRED value="path_rel">
                    <ARGS>
                        <ARG type="Event" value="start(E)"/>
                        <ARG type="ThemRole" value="Theme"/>
                        <ARG type="ThemRole" value="?Initial_Location"/>
                        <ARG type="Constant" value="ch_of_loc"/>
                        <ARG type="VerbSpecific" value="prep"/>
                    </ARGS>
                </PRED>
                <PRED value="path_rel">
                    <ARGS>
                        <ARG type="Event" value="end(E)"/>
                        <ARG type="ThemRole" value="Theme"/>
                        <ARG type="ThemRole" value="Destination"/>
                        <ARG type="Constant" value="ch_of_loc"/>
                        <ARG type="VerbSpecific" value="prep"/>
                    </ARGS>
                </PRED>
                <PRED value="exert_force">
                    <ARGS>
                        <ARG type="Event" value="during(E)"/>
                        <ARG type="ThemRole" value="Agent"/>
                        <ARG type="ThemRole" value="Theme"/>
                    </ARGS>
                </PRED>
                <PRED value="direction">
                    <ARGS>
                        <ARG type="Event" value="during(E)"/>
                        <ARG type="Constant" value="toward"/>
                        <ARG type="ThemRole" value="Theme"/>
                        <ARG type="ThemRole" value="Destination"/>
                    </ARGS>
                </PRED>
                <PRED value="cause">
                    <ARGS>
                        <ARG type="ThemRole" value="Agent"/>
                        <ARG type="Event" value="E"/>
                    </ARGS>
                </PRED>
            </SEMANTICS>
        </FRAME>
        <FRAME>
            <DESCRIPTION descriptionNumber="0.2" primary="NP V NP ADVP" secondary="NP-ADVP-PRED; here/there" xtag=""/>
            <!-- discourse has to provide specific Destination and configuration (Prep) of Theme wrt Destination-->
            <EXAMPLES>
                <EXAMPLE>I dropped the books here.</EXAMPLE>
            </EXAMPLES>
            <SYNTAX>
                <NP value="Agent">
                    <SYNRESTRS/>
                </NP>
                <VERB/>
                <NP value="Theme">
                    <SYNRESTRS/>
                </NP>
                <NP value="Destination">
                    <SYNRESTRS>
                        <SYNRESTR Value="+" type="adv_loc"/>
                    </SYNRESTRS>
                </NP>
            </SYNTAX>
            <SEMANTICS>
                <PRED value="motion">
                    <ARGS>
                        <ARG type="Event" value="during(E)"/>
                        <ARG type="ThemRole" value="Theme"/>
                    </ARGS>
                </PRED>
               <PRED value="path_rel">
                    <ARGS>
                        <ARG type="Event" value="start(E)"/>
                        <ARG type="ThemRole" value="Theme"/>
                        <ARG type="ThemRole" value="?Initial_Location"/>
                        <ARG type="Constant" value="ch_of_loc"/>
                        <ARG type="VerbSpecific" value="prep"/>
                    </ARGS>
                </PRED>
                <PRED value="path_rel">
                    <ARGS>
                        <ARG type="Event" value="end(E)"/>
                        <ARG type="ThemRole" value="Theme"/>
                        <ARG type="ThemRole" value="Destination"/>
                        <ARG type="Constant" value="ch_of_loc"/>
                        <ARG type="VerbSpecific" value="prep"/>
                    </ARGS>
                </PRED>
                <PRED value="exert_force">
                    <ARGS>
                        <ARG type="Event" value="during(E)"/>
                        <ARG type="ThemRole" value="Agent"/>
                        <ARG type="ThemRole" value="Theme"/>
                    </ARGS>
                </PRED>
                <PRED value="direction">
                    <ARGS>
                        <ARG type="Event" value="during(E)"/>
                        <ARG type="Constant" value="toward"/>
                        <ARG type="ThemRole" value="Theme"/>
                        <ARG type="ThemRole" value="Destination"/>
                    </ARGS>
                </PRED>
                <PRED value="cause">
                    <ARGS>
                        <ARG type="ThemRole" value="Agent"/>
                        <ARG type="Event" value="E"/>
                    </ARGS>
                </PRED>
            </SEMANTICS>
        </FRAME>
    </FRAMES>
    <SUBCLASSES/>
</VNCLASS>